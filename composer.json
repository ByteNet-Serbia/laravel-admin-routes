{
    "name": "bytenet/laravel-admin-routes",
    "type": "library",
    "description": "ByteNET Laravel Admin routes package, which offers transportation route manipulations",
    "keywords": [
        "bytenet",
		"laravel",
		"admin",
        "transport",
        "route"
    ],
    "homepage": "https://bitbucket.org/bytenet-serbia/laravel-admin-routes",
    "license": "BTNT-License v1.0",
    "authors": [
        {
            "name": "Nikola Zeravcic",
            "email": "office@bytenet.rs",
            "homepage": "http://www.bytenet.rs",
            "role": "CEO, Chief Architect & Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.3",
        "php" : "7.0.*|~7.1",
        "laravel/framework": "5.3.*",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "ByteNet\\LaravelAdminRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByteNet\\LaravelAdminRoutes\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
